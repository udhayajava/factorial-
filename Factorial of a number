import java.util.Scanner;
public class Factorial
{
    public static void main(String[] args) {
        int num , f = 1;
        Scanner in = new Scanner(System.in);
        System.out.println("Enter the number");
        num = in.nextInt();
        for(int i = 1 ; i <= num ; i++){
            f = f * i;
        }
        System.out.println("The Factorial of " +num+ " is " +f);
    }
}
